<!--MueLu parameters to run Jacobi smoother test-->
<!--These parameters tell MueLu to use a MATLAB function as smoother-->

<ParameterList name="MueLu">
  <ParameterList name="Factories">
    <ParameterList name="myBrickAggFactory">
      <Parameter name="factory" type="string" value="BrickAggregationFactory"/>
      <Parameter name="aggregation: brick x size" type="int" value="3"/>
      <Parameter name="aggregation: brick y size" type="int" value="1"/>
      <Parameter name="aggregation: brick z size" type="int" value="1"/>
    </ParameterList>
  </ParameterList>
  <ParameterList name="Hierarchy">
    <ParameterList name="All">
      <Parameter name="Aggregates" type="string" value="myBrickAggFactory"/>
    </ParameterList>
  </ParameterList>
</ParameterList>
