<!--MueLu parameters to run unsmoothed P MATLAB test-->
<ParameterList name="MueLu">
  <Parameter name="reuse: type" type="string" value="full"/>
  <ParameterList name="Factories">
    <ParameterList name="myAggFactory">
      <Parameter name="factory" type="string" value="UncoupledAggregationFactory"/>
    </ParameterList>
    <ParameterList name="myMatlabPtentFactory">
      <Parameter name="factory" type="string" value="TwoLevelMatlabFactory"/> 
      <Parameter name="Provides" type="string" value="Ptent"/>
      <Parameter name="Needs Fine" type="string" value="Aggregates"/>
      <Parameter name="Needs Coarse" type="string" value=""/>
      <Parameter name="Function" type="string" value="matlabPtentTest"/>
    </ParameterList>
  </ParameterList>
  <ParameterList name="Hierarchy">
    <ParameterList name="All">
      <Parameter name="Ptent" type="string" value="myMatlabPtentFactory"/>
      <Parameter name="Aggregates" type="string" value="myAggFactory"/>
    </ParameterList>
  </ParameterList>
</ParameterList>
